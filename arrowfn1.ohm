parens {
top = text*
text =
    "(" ws* "("? id? moreID* ")"? ws* "=>" ws* "{" ws* text* "}" ws* ")" ws*  -- arrowfn1
  | "(" text* ")"  -- nestedParen
  | "{" text* "}"  -- nestedBrace
  | notBracket     -- notNested
notBracket = ~"(" ~")" ~"{" ~"}" any
id = firstChar restChar*
firstChar = "_" | letter
restChar = "_" | alnum
moreID = "," ws* id ws*
ws = " " | "\t" | "\n"
}

